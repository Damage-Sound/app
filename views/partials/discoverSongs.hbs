{{#each allSongs}}
<div class="song-container">
    <img src="{{cover}}" alt="song cover" class="song-cover">
    <h3 class="song-title">{{name}}</h3>
    <p class="song-plays">{{plays.total}}</p>
    <p class="song-likes">{{likes}}</p>
    <button class="btn btn-dark btn-block btn-lg playButton" media={{url}} songID={{id}}>Play</button>
    <button class="btn btn-dark btn-block btn-lg likeButton" media={{url}} songID={{id}}>Like</button>
</div>
{{/each}}